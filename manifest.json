{
    "name" : "Medchk",
    "version" : "0.0.1",
    "manifest_version" : 2,
    "browser_action" : {
         "default_popup" : "index.html" ,
         "default_icon" : "pharma_design.png"
    },
    "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "nativeMessaging"
  ],
    "icons" : {
        "512" : "pharma_design.png"
    },
      "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}